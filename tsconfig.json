{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["ES6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "ES3",
    "baseUrl": "./src",
    "paths": {
      "screens/*": ["screens/*"],
    }
  }
}